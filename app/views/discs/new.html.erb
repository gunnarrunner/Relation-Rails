<%= form_with url: "/discs_manufacturers/#{@discs_manufacturer.id}/discs", method: :post, local: true do |f| %>
  <%= f.label :name %>
  <%= f.text_field :name %><br>
  <%= f.label :in_production %>
  <%= f.text_field :in_production %><br>
  <%= f.label :speed %>
  <%= f.text_field :speed %><br>
  <%= f.label :discs_manufacturer_id %>
  <%= f.text_field :discs_manufacturer_id %>
  <%= f.submit "Create New Disc" %>
<% end %>
